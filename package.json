{
  "name": "llecoop",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Carlos Matheu Armengol <info@plastikaweb.com> (https://www.plastikaweb.com)",
  "keywords": [
    "angular",
    "ngrx",
    "material",
    "flexbox",
    "firebase"
  ],
  "repository": "git@github.com:plastikaweb/llecoop.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build --prod --build-optimizer --optimization",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint llecoop llecoop-e2e --fix",
    "stylish": "ng lint llecoop llecoop-e2e --format stylish",
    "e2e": "ng e2e --port 4000",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "clean": "rimraf node_modules",
    "clean:all": "yarn clean dist dll && npm cache clean -f",
    "clean:build": "rimraf build",
    "reinstall": "yarn clean:all && rm -f package-lock.json && rm -f yarn.lock && yarn"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.1.8",
    "@angular/compiler": "^6.1.8",
    "@angular/core": "^6.1.8",
    "@angular/flex-layout": "6.0.0-beta.18",
    "@angular/forms": "^6.1.8",
    "@angular/http": "^6.1.8",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.1.8",
    "@angular/platform-browser-dynamic": "^6.1.8",
    "@angular/router": "^6.1.8",
    "@covalent/core": "2.0.0-beta.3",
    "@firebase/app": "~0.3.3",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/router-store": "6.1.0",
    "@ngrx/schematics": "^6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@ngx-formly/core": "4.7.2",
    "@ngx-formly/material": "4.7.2",
    "angularfire2": "^5.0.0-rc.11",
    "core-js": "^2.5.4",
    "firebase": "^5.2.0",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.3.1",
    "rxjs": "^6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^7.0.0",
    "@angular-devkit/build-angular": "~0.8.3",
    "@angular/cli": "^6.2.3",
    "@angular/compiler-cli": "^6.1.8",
    "@angular/language-service": "^6.1.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.9",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "commitizen": "^3.0.4",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.6.0",
    "lodash": "^4.17.10",
    "ngrx-store-freeze": "^0.2.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.3",
    "opencollective": "^1.0.3",
    "protractor": "^5.4.0",
    "rimraf": "^2.6.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  },
  "config": {
    "ghooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "run-s lint test:coverage e2e build"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
