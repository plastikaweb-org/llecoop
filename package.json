{
  "name": "llecoop",
  "version": "0.0.0",
  "license": "MIT",
  "author": {
    "email": "info@plastikaweb.com",
    "name": "Carlos Matheu Armengol",
    "url": "https://www.plastikaweb.com"
  },
  "keywords": [
    "angular",
    "ngrx",
    "material",
    "flexbox",
    "firebase"
  ],
  "repository": "git@github.com:plastikaweb/llecoop.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build --prod",
    "test": "ng test",
    "test:single": "ng test --code-coverage --watch false",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "stylish": "ng lint --format stylish",
    "e2e": "ng e2e",
    "precommit": "run-s lint test:single e2e",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "clean": "rimraf node_modules",
    "clean:all": "npm run clean -- dist dll && npm cache clean",
    "clean:build": "rimraf build",
    "reinstall": "npm run clean -- && rm -f package-lock.json && npm install"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.1.0",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "6.0.0-beta.15",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.1.0",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@covalent/core": "2.0.0-beta.1",
    "@firebase/app": "~0.1.0",
    "@ngrx/effects": "6.0.1",
    "@ngrx/entity": "6.0.1",
    "@ngrx/router-store": "6.0.1",
    "@ngrx/store": "6.0.1",
    "@ngrx/store-devtools": "6.0.1",
    "@ngx-formly/core": "4.3.0",
    "@ngx-formly/material": "4.3.0",
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.5.5",
    "firebase": "5.0.4",
    "hammerjs": "^2.0.8",
    "jasmine-marbles": "^0.3.1",
    "rxjs": "^6.2.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/cli": "^6.0.5",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/language-service": "^6.0.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.2.1",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.2.2",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  },
  "config": {
    "commitizen": {
